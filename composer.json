{
    "name": "ndg0/laravel-socialite-auth",
    "description": "A quick drop-in solution for authenticating users with Laravel socialite.",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Plakhin",
            "email": "stan@ndgo.dev",
            "homepage": "https://ndgo.dev"
        }
    ],
    "homepage": "https://github.com/ndg0/laravel-socialite-auth",
    "keywords": ["Laravel", "Socialite", "Auth"],
    "require": {
        "illuminate/support": "~7|~8",
        "laravel/socialite": "^5.1",
        "socialiteproviders/google": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ndg0\\LaravelSocialiteAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ndg0\\LaravelSocialiteAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndg0\\LaravelSocialiteAuth\\LaravelSocialiteAuthServiceProvider"
            ]
        }
    }
}
